<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="history" catalog="surgicalpathologyspecimens"
		class="com.sps.entity.History">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="patient" catalog="surgicalpathologyspecimens"
		class="com.sps.entity.Patient">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="specimen" catalog="surgicalpathologyspecimens"
		class="com.sps.entity.Specimen">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="user" catalog="surgicalpathologyspecimens"
	class="com.sps.entity.User">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>